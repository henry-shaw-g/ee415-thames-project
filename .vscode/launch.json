{
    "version": "0.2.0",
    "configurations": [
        {
            // debugpy config for python file debug session
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}"
        }
    ]
}